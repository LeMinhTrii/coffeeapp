{
    "builds": [{
        "src": "main.jsx",
        "use": "@vercel/static"
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }, {
            "src": "/.*",
            "dest": "main.jss"
        }
    ],
    "public": true
}